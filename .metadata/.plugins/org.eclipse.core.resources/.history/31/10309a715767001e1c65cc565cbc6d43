package com.controller;

public class IfDemo {
	public static void main(String[] args) {
		
	}
	
	public void identifyStatus(int age){
		if(age >= 60) {
			System.out.println("Senior citizen");
		}
		else {
			System.out.println("not senior");
		}
	}
	
	public double computeDiscount(int age){
		if(age>=60) {
			return 5.0; 
		}
			
		return 0; 
	}
}
